<div class="toggleable schema-attribute{{if .Partial "Required"}} required{{end}}{{if .Partial "Optional"}} optional{{end}}">
  <input id="toggle-{{.Partial "TagName" | render}}" type="checkbox" class="toggle"{{if .Partial "Required"}} checked{{end}} />
  <label for="toggle-{{.Partial "TagName" | render}}" class="attribute-header">
    {{.Partial "Targets" | render}}

    <pre><span class="attribute-name">{{.Partial "Attribute" | render}}</span>: <span class="attribute-type">{{.Partial "Type" | render}}</span></pre>
  </label>

  <div class="toggle-content attribute-content">
    {{.Content | render}}
  </div>
</div>
